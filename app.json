{
  "expo": {
    "name": "Juego",
    "slug": "Juego",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#17344f"
    },
    "updates": {
